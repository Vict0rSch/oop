{
    "name" :"Media graph",
    "manifest_version": 2,
    "version" :"1.0",
    "description" :"media-viz",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "/html/login.html"
    },
    "background": {
        "page": "/html/background.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "https://localhost:8000/*",
        "https://fr.wikipedia.org/*",
        "tabs",
        "notifications"
    ]
}